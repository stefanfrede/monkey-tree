{
  "name": "monkey-tree",
  "version": "1.0.0",
  "description": "Simple Polymer 3 tree component",
  "main": "monkey-tree.js",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "npm run lint:javascript && npm run lint:polymer",
    "lint:javascript": "npx eslint .",
    "lint:polymer": "polymer lint",
    "start": "polymer serve --open",
    "test": "polymer test"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:stefanfrede/monkey-tree.git"
  },
  "keywords": [
    "tree",
    "monkey-tree",
    "web-component",
    "web-components",
    "polymer"
  ],
  "author": "Stefan Frede <public@frede.biz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stefanfrede/monkey-tree/issues"
  },
  "homepage": "https://github.com/stefanfrede/monkey-tree#readme",
  "dependencies": {
    "@material/mwc-icon": "^0.1.2",
    "@polymer/lit-element": "^0.5.2",
    "@polymer/polymer": "^3.0.2",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@polymer/iron-component-page": "^3.0.0-pre.21",
    "@polymer/iron-demo-helpers": "^3.0.0-pre.21",
    "@webcomponents/webcomponentsjs": "^2.0.2",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "wct-browser-legacy": "^1.0.1"
  }
}
